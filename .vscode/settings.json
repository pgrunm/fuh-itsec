{

    "latex-workshop.latex.tools": [
        {
            "name": "latexmk",
            "command": "latexmk",
            "args": [
                "-synctex=1",
                "-interaction=nonstopmode",
                "-file-line-error",
                "--shell-escape",
                "-pdf",
                // "-auxdir=./augs",
                "-auxdir=%DIR%/augs",
                "%DOC%"
            ]
        }
    ],
}